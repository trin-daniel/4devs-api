{
  "name": "4devs-api",
  "version": "1.0.0",
  "description": "",
  "main": "Dist/Main/Server.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "jest --passWithNoTests --runInBand --watch -c jest-unit-config.js",
    "test:integration": "jest --passWithNoTests  --runInBand --watch -c jest-integration-config.js",
    "test:ci": "jest --passWithNoTests --runInBand --coverage",
    "test:staged": "jest --passWithNoTests --runInBand --findRelatedTests",
    "test:coveralls": "npm run test:ci && coveralls < Coverage/lcov.info",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "start": "DEBUG=app:server node Dist/Main/Server.js",
    "start:dev": "DEBUG=app* node Dist/Main/Server.js",
    "build": "rimraf Dist && tsc -p build.json",
    "postbuild": "copyfiles -u 1 Public/**/* Dist/Static"
  },
  "keywords": [],
  "author": "Carlos Daniel",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.4.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^3.6.4",
    "swagger-ui-express": "^4.1.6"
  },
  "engines": {
    "node": "15.x"
  }
