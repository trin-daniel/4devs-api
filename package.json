    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.6",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
