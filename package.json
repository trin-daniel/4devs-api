{
  "name": "4devs-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/server.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "jest --passWithNoTests --runInBand --watch -c jest-unit-config.js",
    "test:integration": "jest --passWithNoTests  --runInBand --watch -c jest-integration-config.js",
    "test:ci": "jest --passWithNoTests --runInBand --coverage",
    "test:staged": "jest --passWithNoTests --runInBand --findRelatedTests",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "start": "DEBUG=app:server node dist/main/server.js",
    "start:dev": "DEBUG=app* node dist/main/server.js",
    "build": "rimraf dist && tsc"
  },
